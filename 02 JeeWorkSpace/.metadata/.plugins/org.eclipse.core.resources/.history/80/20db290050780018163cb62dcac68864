-- TAB : address
-- addr_seq (DECIMAL, PRIMARY KEY)
-- zipcode
-- province 경기도
-- city 서울시, 안양시
-- gugun 강남구, 가평군
-- doro 백범로 15길

CREATE TABLE address(
	addr_seq DECIMAL PRIMARY KEY,
	zip_code VARCHAR2(20),
	province VARCHAR2(20),
	city VARCHAR2(20),
	gugun VARCHAR2(20),
	doro VARCHAR2(20)
);

INSERT INTO address
(
	addr_seq, zip_code, province, city, gugun, doro
)
VALUES
(
	1, '123456', '경기도', '서울시', '강남구', '백범로 15길'
);

SELECT * FROM address;

